<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.daljin.daljinnasandroid">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>

    <application
            android:allowBackup="true"
            android:icon="@drawable/daljin_logo_vertical"
            android:label="DaljinNAS"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity
                android:name="com.com.daljin.daljinnasandroid.PerActivity"
                android:label="@string/title_activity_per"
                android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity android:name=".SingupActivity">
        </activity>
        <activity
                android:name=".UploadActivity"
                android:label="">
        </activity>
        <activity android:name=".TestActivity">
        </activity>
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
                android:name=".FileActivity"
                android:label="">
        </activity>
        <activity android:name=".LoginActivity">
        </activity>

        <service
                android:name=".DownloadService"
                android:enabled="true">
        </service>
        <service
                android:name=".UploadService"
                android:enabled="true">
        </service>
    </application>

</manifest>